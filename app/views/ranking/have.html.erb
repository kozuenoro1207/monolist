<div class="ranking">
<h1>Have Ranking</h1>
<% if @rank_haves_item.nil? %>
    Haveされている商品はありません
<% else %>
    <% @rank_haves_item.each do |t| %>
       <div class="one-rank" >
           <div class="rank-icon">
            <%= @rank_haves_id.index(t.id)+1 %>
           </div>
           <div class="rank-item">
            <div class="title">
              <% unless current_page?(t) %>
                <%= link_to  t.title , t  %>
              <% end %>
            </div>
        </div>
       </div>
    <% end %>
<% end %>
</div>